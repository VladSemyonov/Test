    "gatsby-plugin-sass": "^4.7.1",
    "gatsby-plugin-sharp": "^3.7.1",
    "gatsby-plugin-sitemap": "^4.3.1",
    "gatsby-plugin-typescript": "^3.7.1",
    "gatsby-plugin-webfonts": "^2.1.0",
    "gatsby-source-filesystem": "^3.7.1",
    "gatsby-source-graphql": "^3.7.1",
    "gatsby-theme-i18n": "^2.0.0",
    "gatsby-theme-i18n-react-i18next": "^2.0.0",
    "gatsby-transformer-json": "^3.7.1",
    "gatsby-transformer-remark": "^4.4.1",
    "gatsby-transformer-sharp": "^3.7.1",
    "i18next": "^20.3.0",
    "i18next-browser-languagedetector": "^6.1.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.9.0",
    "sanitize-html": "^2.4.0"
  },
