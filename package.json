{
  "name": "yunotex.com",
  "version": "1.0.15",
  "private": true,
  "description": "yunotex.com",
  "author": "yunotex.com",
  "keywords": [
    "yunotex.com"
  ],
  "scripts": {
    "clean": "gatsby clean",
    "develop": "gatsby develop --open",
    "build": "gatsby clean && gatsby build",
    "build-prefix-links": "gatsby build --prefix-links",
    "deploy-prod": "npm version patch && gatsby clean && gatsby build && serverless deploy --force --stage=prod -c=serverless.yml",
    "deploy-prod-test": "gatsby clean && gatsby build && serverless deploy --force --stage=prod -c=serverless.yml",
    "deploy-dev": "gatsby clean && gatsby build && serverless deploy --force --stage=dev -c=serverless.yml",
    "sls-deploy-prod": "serverless deploy --force --stage=prod -c=serverless.yml",
    "sls-deploy-dev": "serverless deploy --force --stage=dev -c=serverless.yml",
    "serve": "gatsby serve --open",
    "type-check": "tsc --noEmit",
    "lint": "tsc --noEmit && eslint . --ignore-path .gitignore --ext .ts,.tsx,.js"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "classnames": "^2.3.1",
    "fast-deep-equal": "^3.1.3",
